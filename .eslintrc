    "no-console": "off",
    "prettier/prettier": ["error", { "singleQuote": true }]
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
